## hw

library(nycflights13)
library(tidyverse) 

#ask5questions this dataset
data("flights")
View(flights)

#Q1. most flight carrier in Sep 2013
flights %>%
  filter(month == 9,year == 2013) %>%
  count(carrier)
flights %>%
  filter(month == 9,year == 2013) %>%
  count(carrier) %>%
  arrange(desc(n))
head(5)

#Q2
flights %>%
  select(origin) %>%
  filter(grepl("A",origin))

#Q3
flights %>%
  select(distance) %>%
  arrange(distance,distance)
